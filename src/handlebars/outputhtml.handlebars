<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="utf-8" />
		<meta name="title" content="{{name}}" />
		<meta name="description" content="{{description}}" />
		<meta name="copyright" content="{{copyright}}" />
		<meta name="timestamp" content="{{timestamp}}" />
		<meta name="author" content="https://video.to-scorm.com" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>{{name}}</title>
		<script crossorigin="anonymous" src="https://polyfill.io/v3/polyfill.min.js"></script>
{{#if isMediaElement}}
		<link href="https://cdnjs.cloudflare.com/ajax/libs/mediaelement/4.2.11/mediaelementplayer.min.css" rel="stylesheet" type="text/css">
		<script src="https://cdnjs.cloudflare.com/ajax/libs/mediaelement/4.2.11/mediaelement-and-player.js" type="text/javascript"></script>
{{else}}
		<link href="https://cdn.plyr.io/3.5.6/plyr.css" rel="stylesheet" type="text/css">
		<script src="https://cdn.plyr.io/3.5.6/plyr.js" type="text/javascript"></script>
{{/if}}
		<link href="style.css" rel="stylesheet" type="text/css">
		<script type="text/javascript">{{{playerApi}}}</script>
{{#if analytics}}
		<script>
			(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
			(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
			m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
			})(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
			ga('create','{{analytics}}','auto');ga('set','anonymizeIp',true);
			document.addEventListener("DOMContentLoaded",function(dom){document.body.addEventListener('pageview',function(event){
			ga('send','event','{{name}}','page',event.detail,{nonInteraction:true});},false)});
		</script>
{{/if}}
	</head>
	<body>
		<div id="frameContainer">
{{#if isVideo}}
			<video id="player"></video>
{{else}}
			<audio id="player"></audio>
{{/if}}
		</div>
		<footer>
			<div>
				{{#if copyright}}<p>{{copyright}}</p>{{/if}}
				<p class="watermark">Created using <a href="https://video.to-scorm.com" target="_blank">Video 2 Scorm</a>.</p>
			</div>
		</footer>
		<script src="run.js" type="text/javascript"></script>
	</body>
</html>